    "log4js": "^6.9.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/amqplib": "^0.10.6",
    "@types/express": "^5.0.0",
    "@types/inversify": "^2.0.33",
    "@types/jest": "^29.5.14",
    "@types/log4js": "^2.3.5",
    "@types/node": "^22.12.0",
    "@typescript-eslint/parser": "^8.22.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
